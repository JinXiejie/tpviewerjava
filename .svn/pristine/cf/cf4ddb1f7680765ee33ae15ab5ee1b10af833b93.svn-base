package com.mip.tp.data.gaswater;

/**
 * 
 * @author longyz
 * @date 2012-4-20
 */
public class Channel {
	
	public int peakcount;//峰个数
	public int datacount;//数据点个数
	public int sampletime = 300;//采样时间,dat文件无法读取到,现在写死,300测试是符合样例中的值的
	public double xMaxValue;//x轴最大值,对应flash的xvalue
	public double yMaxValue;//y轴最大值,对应flash的yvalue
	public ChannelPeak[] peaks;//通道峰值详细信息
	public float[] datas;//通道数据值
	
	/**
	 * y轴最大坐标值
	 */
	public double maxYCoordinate = 0;
	/**
	 * y轴最小坐标值
	 */
	public double minYCoordinate = 0;
	/**
	 * x轴最小坐标值
	 */
	public double maxXCoordinate = 0;
	/**
	 * x轴最小坐标值
	 */
	public double minXCoordinate = 0;
}
